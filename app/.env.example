# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Gemini API Key
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_gemini_api_key
GEMINI_API_KEY=your_gemini_api_key
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key

# Apollo.io API Key for Lead Finding
# Get from: https://app.apollo.io/#/settings/integrations/api
APOLLO_API_KEY=your_apollo_api_key
NEXT_PUBLIC_APOLLO_API_KEY=your_apollo_api_key

# Modal Processor Endpoint (optional - for async batch processing)
MODAL_API_URL=your_modal_endpoint_or_leave_empty

# Google Sheets Integration (OAuth)
# Get from: https://console.cloud.google.com/apis/credentials
# Enable: Google Sheets API and Google Drive API
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_oauth_client_id
GOOGLE_CLIENT_SECRET=your_google_oauth_client_secret
# Note: NEXT_PUBLIC_GOOGLE_API_KEY can reuse GOOGLE_API_KEY above if same project
NEXT_PUBLIC_GOOGLE_API_KEY=your_google_api_key
